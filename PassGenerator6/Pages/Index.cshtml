@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<p>Data de Nascimento: 16/10/2006</p>
<img src="data:image/jpeg;base64,"/>

<a href="https://g1.globo.com/">Clique aqui para ir em um site do G1</a>

<div class="text-center">
    <button id="generatePasswordBtn" class="button-85" onclick="generatePass()">Botão de Teste</button>
</div>

<style>

    img{
        width: 360px;
    }

    a{
        text-decoration: none;
        color:white
    }

    p{
        font-weight: bold;
        font-size: 40px;
        font-weight: 600;
        color: white;
    }

    a:hover{
        font-weight:700;
    }

    .text-center{
        line-break: revert-layer;
        word-break: break-all;
    }

    body{
        background-color: skyblue;
    }

    h1{
        font-weight: bold;
        font-size: 40px;
        font-weight: 600;
        color:white;
    }

    h2{
        font-weight: bold;
        font: 20px;
        font-weight: 400;
        color: white;
    }

    /* CSS */
    .button-85 {
      padding: 0.6em 2em;
      border: none;
      outline: none;
      color: rgb(255, 255, 255);
      background: #111;
      cursor: pointer;
      position: relative;
      z-index: 0;
      border-radius: 10px;
      user-select: none;
      -webkit-user-select: none;
      touch-action: manipulation;
    }

    .button-85:before {
      content: "";
      background: linear-gradient(
        45deg,
        #ff0000,
        #ff7300,
        #fffb00,
        #48ff00,
        #00ffd5,
        #002bff,
        #7a00ff,
        #ff00c8,
        #ff0000
      );
      position: absolute;
      top: -2px;
      left: -2px;
      background-size: 400%;
      z-index: -1;
      filter: blur(5px);
      -webkit-filter: blur(5px);
      width: calc(100% + 4px);
      height: calc(100% + 4px);
      animation: glowing-button-85 20s linear infinite;
      transition: opacity 0.3s ease-in-out;
      border-radius: 10px;
    }   

    .button-85:after {
      z-index: -1;
      content: "";
      position: absolute;
      width: 100%;
      height: 100%;
      background: #222;
      left: 0;
      top: 0;
      border-radius: 10px;
    }
</style>

@section Scripts {
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script>
        function generatePass() {
            var passLength = $("#passLength").val();
            $.ajax({
                url: '@Url.Page(null, "GenerateRandomPassword")',
                type: "GET",
                data: { passLength: passLength },
                success: function (password) {
                    $("#passwordDisplay").text("Random Password: " + password);
                }
            });
        };
    </script>

   
}



